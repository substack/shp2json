{
    "name" : "shp2json",
    "description" : "Convert shapefile zip archives into GeoJSON using ogr2ogr with a streaming interface",
    "version" : "0.1.2",
    "repository" : {
        "type" : "git",
        "url" : "git://github.com/substack/shp2json.git"
    },
    "main" : "index.js",
    "bin" : {
        "shp2json" : "bin/cli.js"
    },
    "keywords" : [
        "shapefile",
        "shp",
        "zip",
        "stream",
        "convert",
        "geojson",
        "geo",
        "gis",
        "json"
    ],
    "directories" : {
        "lib" : ".",
        "example" : "example",
        "test" : "test"
    },
    "scripts" : {
        "test" : "tap test/*.js"
    },
    "dependencies" : {
        "seq" : "0.3.x",
        "morestreams" : "0.1.x",
        "findit" : "0.1.x"
    },
    "devDependencies" : {
        "tap" : "0.0.x"
    },
    "engines" : {
        "node" : ">=0.4.0"
    },
    "license" : "MIT",
    "author" : {
        "name" : "James Halliday",
        "email" : "mail@substack.net",
        "url" : "http://substack.net"
    }
}
